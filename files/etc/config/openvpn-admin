
config settings 'global'
	option openvpn_instance 'myvpn'
	option openvpn_config_path '/etc/config/openvpn'
	option refresh_interval '1'
	option history_size '20'
	option blacklist_duration '300'
	option log_file '/tmp/openvpn.log'
	option history_file '/etc/openvpn-admin/openvpn_connection_history.json'
	option blacklist_file '/etc/openvpn-admin/blacklist.json'
	option easyrsa_dir '/etc/easy-rsa'
	option easyrsa_pki '/etc/easy-rsa/pki'
	option openvpn_pki '/etc/openvpn/pki'
	option logs_refresh_interval '10'
	option logs_display_lines '100'
	option generate_client_script '/etc/openvpn-admin/generate-client.sh'
	option renew_cert_script '/etc/openvpn-admin/renewcert.sh'
	option blacklist_enabled '1'
	option logs_refresh_enabled '1'
	option refresh_enabled '1'
	option temp_dir '/tmp/openvpn-admin'
	option clean_garbage_enabled '0'
	option clean_garbage_time '4:50'
	option clean_garbage_script '/etc/openvpn-admin/clean-garbage.sh'
	option server_template_path '/etc/openvpn-admin/template/server.template'

